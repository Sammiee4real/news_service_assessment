{% extends 'base.html.twig' %}

{% block title %}DownloadedNews{% endblock %}

{% block body %}
    <h1>DownloadedNews</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ downloaded_news.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ downloaded_news.title }}</td>
            </tr>
            <tr>
                <th>Short_description</th>
                <td>{{ downloaded_news.shortDescription }}</td>
            </tr>
            <tr>
                <th>Picture_path</th>
                <td>{{ downloaded_news.picturePath }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_downloaded_news_index') }}">back to list</a>

    <a href="{{ path('app_downloaded_news_edit', {'id': downloaded_news.id}) }}">edit</a>

    {{ include('downloaded_news/_delete_form.html.twig') }}
{% endblock %}
